{
	"name": "studio-template",
	"private": true,
	"version": "1.0.0",
	"main": "package.json",
	"license": "UNLICENSED",
	"scripts": {
		"dev": "sanity dev",
		"start": "sanity start",
		"prebuild": "npm run extract-types",
		"build": "sanity build",
		"deploy": "sanity deploy",
		"clean": "rm -f ./schema.json && rm -f ./sanity.types.ts && rm -rf ./.sanity",
		"typegen": "bun run extract-types && bun run generate-types",
		"extract-types": "sanity schema extract --enforce-required-fields",
		"generate-types": "sanity typegen generate"
	},
	"keywords": [
		"sanity",
		"CMS"
	],
	"dependencies": {
		"@heroicons/react": "^2.2.0",
		"@iconify/react": "^5.2.1",
		"@sanity/icons": "^3.7.4",
		"@sanity/vision": "^3.95.0",
		"@tailwindcss/vite": "^4.1.11",
		"@types/pluralize": "^0.0.33",
		"date-fns": "^3.6.0",
		"lodash": "^4.17.21",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"rxjs": "^7.8.2",
		"sanity": "^3.95.0",
		"sanity-plugin-iconify": "^2.0.2",
		"sanity-plugin-media": "^3.0.3",
		"shared-lib": "workspace:*",
		"slugify": "^1.6.6",
		"styled-components": "^6.1.19",
		"tailwindcss": "^4.1.11",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"overrides": {
		"react": "$react",
		"react-dom": "$react-dom"
	},
	"devDependencies": {
		"@sanity/eslint-config-studio": "^5.0.2",
		"@types/lodash": "^4.17.19",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@types/react-is": "^19.0.0",
		"eslint": "^9.30.0",
		"prettier": "^3.6.2",
		"typescript": "^5.8.3"
	},
	"prettier": {
		"semi": false,
		"printWidth": 100,
		"bracketSpacing": false,
		"singleQuote": true
	}
}
